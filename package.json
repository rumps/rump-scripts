{
  "name": "rump-scripts",
  "version": "0.8.1",
  "author": "Juan Placencia",
  "description": "Rump piece for building JS with Webpack",
  "license": "MIT",
  "homepage": "https://github.com/rumps/scripts",
  "main": "./lib",
  "repository": {
    "type": "git",
    "url": "https://github.com/rumps/scripts"
  },
  "keywords": [
    "babel",
    "bower",
    "coffee-script",
    "gulp",
    "javascript",
    "rump",
    "rump-module",
    "webpack"
  ],
  "scripts": {
    "codecov": "npm run coverage",
    "postcodecov": "codecov < coverage/coverage.json",
    "precompile": "[ -d lib ] && trash lib || true",
    "compile": "babel src --out-dir lib --source-maps=inline",
    "coverage": "babel-node node_modules/isparta/bin/isparta cover --report json --report html --report text node_modules/mocha/bin/_mocha -- -R min",
    "lint": "eslint src test",
    "pub": "npm run test && npm run compile && npm publish",
    "pretest": "npm run lint",
    "test": "mocha"
  },
  "dependencies": {
    "babel-runtime": "^5.8.20",
    "extend": "^3.0.0",
    "globule": "^0.2.0",
    "gulp-util": "^3.0.6",
    "webpack": "~1.11.0"
  },
  "devDependencies": {
    "babel": "^5.8.19",
    "babel-eslint": "^4.0.5",
    "bluebird": "^2.9.34",
    "buffer-equal": "^0.0.1",
    "codecov.io": "^0.1.5",
    "coffee-loader": "^0.7.2",
    "coffee-script": "^1.9.3",
    "convert-source-map": "^1.1.1",
    "eslint": "^1.0.0",
    "graceful-fs": "^4.1.2",
    "gulp": "^3.9.0",
    "html-loader": "^0.3.0",
    "isparta": "^3.0.3",
    "json-loader": "^0.5.2",
    "lodash": "^3.10.1",
    "mocha": "^2.2.5",
    "mz": "^2.0.0",
    "riot": "^2.2.4",
    "riotjs-loader": "^1.1.3",
    "rump": "^0.8.0",
    "should": "^7.0.2",
    "should-sinon": "0.0.3",
    "sinon": "^1.15.4",
    "timeout-then": "^1.0.0",
    "trash": "^2.0.0"
  }
}
